@article{Besley2001,
abstract = {This paper studies the impact of lobbying on political competition and policy outcomes in a framework which integrates the citizen-candidate model of representative democracy with the menu-auction model of lobbying. Positive and normative issues are analysed. On the positive side, lobbying need have little or no effect on policy outcomes because voters can restrict the influence of lobbyists by supporting candidates with offsetting policy preferences. On the normative side, coordination failure among lobbyists can result in Pareto inefficient policy choices. In addition, by creating rents to holding office, lobbying can lead to “excessive” entry into electoral competition.},
