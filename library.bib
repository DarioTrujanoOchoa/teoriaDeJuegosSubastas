journal = {Brain Research},
keywords = {algorithms,animal,animals,behavior,competitive behavior,competitive behavior physiology,decision making,decision making physiology,feedback,learning,learning physiology,logistic models,macaca mulatta,male,models,probability,psychological,reinforcement (psychology),time factors},
