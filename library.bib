title = {{Peer contagion in child and adolescent social and emotional development.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3523739{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {62},
year = {2011}
}
