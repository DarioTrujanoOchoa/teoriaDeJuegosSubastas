pmid = {21782849},
publisher = {Elsevier B.V.},
title = {{The effects of motivation on response rate: a hidden semi-Markov model analysis of behavioral dynamics.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21782849},
volume = {201},
year = {2011}
}
