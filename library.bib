file = {:C$\backslash$:/Users/dtruj/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nilsson, Rieskamp, Wagenmakers - 2011 - Hierarchical Bayesian parameter estimation for cumulative prospect theory.pdf:pdf},
issn = {00222496},
journal = {Journal of Mathematical Psychology},
keywords = {Decision making,Loss aversion,Maximum likelihood,Shrinkage,WinBUGS},
number = {1},
pages = {84--93},
publisher = {Elsevier Inc.},
title = {{Hierarchical Bayesian parameter estimation for cumulative prospect theory}},
