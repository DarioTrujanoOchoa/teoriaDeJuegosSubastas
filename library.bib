journal = {Journal of the Experimental Analysis of Behavior},
title = {{CHOICE, TIME AND FOOD: CONTINUOUS CYCLICAL CHANGES IN FOOD PROBABILITY
BETWEEN REINFORCERS}},
volume = {9999},
year = {2014}
}
