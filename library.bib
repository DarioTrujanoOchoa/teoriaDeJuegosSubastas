journal = {Journal of the experimental analysis of behavior},
keywords = {Animals,Behavior, Animal,Choice Behavior,Columbidae,Competitive Behavior,Discrimination Learning,Game Theory,Models, Psychological,Reinforcement (Psychology)},
month = {mar},
