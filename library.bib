title = {{Multiple medically unexplained physical symptoms and health care utilization: outcome of psychological intervention and patient-related predictors of change.}},
url = {http://dx.doi.org/10.1016/j.jpsychores.2004.02.012},
volume = {57},
year = {2004}
}
@article{Rachlin1981,
abstract = {Maximization theory provides techniques for predicting the behavior of humans and animals. A theoretical behavioral space is constructed in which each point represents a combination of behavioral alternatives; a particular environmental situation can be described as a constraint on the available points within the space. Since maximization theory assumes that Ss always choose the available point with the highest numerical value, the task of the theory is to assign values that remain constant across environmental situations. Maximization theory is an alternative to reinforcement theory as a description of steady-state behavior and can be used to describe laboratory tasks such as rat leverpressing and extended to explain human economic behavior and self-control. It is argued that maximization theory has greater predictive power than reinforcement theory because it views behavior as the quantitative outcome of the interaction of the instrumental response, reinforcer, and the other alternatives available in the situation. Comments by 19 authors on the article, and the authors' response, are included. (3 p ref) (PsycLIT Database Copyright 1982 American Psychological Assn, all rights reserved)},
