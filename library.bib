file = {::},
issn = {1938-3711},
journal = {Journal of the experimental analysis of behavior},
keywords = {Animals,Choice Behavior,Conditioning, Operant,Linear Models,Male,Motivation,Rats,Rats, Wistar,Reinforcement Schedule,Time Perception},
month = {may},
number = {3},
pages = {293--317},
pmid = {19949489},
title = {{Dynamics of choice: relative rate and amount affect local preference at three different time scales.}},
